  ARel = .Implant_Femoral_Head_Axial_Fitting_Surf_Node.ARel
          * RotMat(pi, x) 
          * RotMat((SEG_SIGN)*(-30)/180*pi, y)
          * RotMat((-5)/180*pi, x)
          ;
  
